@model NFI.Models.Application1Dto


<h3>Application1 Detail</h3>

<form class="form-horizontal" role="form">
    <div class="form-group">
        <label class="col-sm-2 text-right">Name:</label>
        <div class="col-sm-10">
            <span>@Html.DisplayTextFor(model => model.Name) </span>
        </div>
    </div>

    <div class="form-group">
        <label class="col-sm-2 text-right">Sex:</label>
        <div class="col-sm-10">
            <span>@Html.DisplayTextFor(model => model.Sex) </span>
        </div>
    </div>

    <div class="form-group">
        <label class="col-sm-2 text-right">Email:</label>
        <div class="col-sm-10">
            <span>@Html.DisplayTextFor(model => model.Email) </span>
        </div>
    </div>

    <div class="form-group">
        <label class="col-sm-2 text-right">Company:</label>
        <div class="col-sm-10">
            <span >@Html.DisplayTextFor(model => model.Company) </span>
        </div>
    </div>

    <div class="form-group">
        <label class="col-sm-2 text-right">Create Date:</label>
        <div class="col-sm-10">
            <span>@Html.DisplayTextFor(model => model.CreateDate) </span>
        </div>
    </div>

    <div class="form-group">
        <label class="col-sm-2 text-right">Attachments:</label>
        <div class="col-sm-10">
            @Html.ActionLink("Download File", "DownloadZipFile", new {appId = Model.AppId})
        </div>
    </div>

</form>







